<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>
  
	
  <servlet>
  	<servlet-name>MasterServlet</servlet-name>
  	<display-name>MasterServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.servlets.MasterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ReimbursementApprovalServlet</servlet-name>
  	<display-name>ReimbursementApprovalServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.servlets.ReimbursementApprovalServlet</servlet-class>
  </servlet>

  <servlet-mapping>
  	<servlet-name>MasterServlet</servlet-name>
  	<url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ReimbursementApprovalServlet</servlet-name>
  	<url-pattern>/ReimbursementApprovalServlet</url-pattern>
  </servlet-mapping>
  
  	<welcome-file-list>
		<welcome-file>./ERS_out.jsp</welcome-file>
	</welcome-file-list>
  
</web-app>
